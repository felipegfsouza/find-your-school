<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DBDBDB"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".TelaMenu" >

    <ImageButton
        android:id="@+id/btnsaoroque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/btnembu"
        android:layout_marginLeft="24dp"
        android:layout_toRightOf="@+id/btnembu"
        android:src="@drawable/saoroque" />

    <WebView
        android:id="@+id/webView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignLeft="@+id/btncotia"
        android:layout_below="@+id/btncotia"
        android:layout_marginTop="48dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="42dp"
        android:text="VEJA O MAPA DE SUA ETEC" />

    <ImageButton
        android:id="@+id/btncotia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_marginRight="18dp"
        android:layout_marginTop="29dp"
        android:layout_toLeftOf="@+id/btnembu"
        android:src="@drawable/cotia" />

    <ImageButton
        android:id="@+id/btnembu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/webView1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/embu" />

</RelativeLayout>